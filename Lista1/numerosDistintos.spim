.text
elemDistintos:
    li $v0, 0
    beq $a1, $zero, encerra

    lw $t0, 0($a0)
    addi $v0, $v0, 1
    addi $a1, $a1, -1
    addi $a0, $a0, 4
    
loop: 
    beq $a1, $zero, encerra
    lw $t1, 0($a0)
    
    bne $t0, $t1, somador

    addi $a1, $a1, -1
    addi $a0, $a0, 4
    j loop

somador:
    addi $v0, $v0, 1
    move $t0, $t1
    addi $a1, $a1, -1
    addi $a0, $a0, 4
    j loop

encerra:
    jr $ra